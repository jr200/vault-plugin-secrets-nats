{
  "useSigningKey": "opsk1",
  "claims": {
    "name": "auth",
    "account": {
      "signingKeys": [
        "auth-sask1"
      ],
      "authorization": {
        "auth_users": [
          "auth"
        ],
        "allowed_accounts": [
          "*"
        ]
      },
      "limits": {
        "subs": -1,
        "conn": -1,
        "leafNodeConn": -1,
        "data": -1,
        "payload": -1,
        "wildcardExports": true,
        "imports": -1,
        "exports": -1
      }
    }
  }
}